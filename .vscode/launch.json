{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Transfer-Debug",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/demos/node",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/tsx",
      "program": "${workspaceFolder}/demos/node/src/index.ts",
      "runtimeArgs": ["--tsconfig", "${workspaceFolder}/demos/node/tsconfig.debug.json"],
      "args": ["transfer-debug"],
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
